
{
  "name": "api",
  "root": "apps/api",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node -r ts-node/register -r tsconfig-paths/register apps/api/src/main.ts",
        "cwd": "."
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/api/tsconfig.app.json"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --config apps/api/jest.config.ts"
      }
    }
  }
}
